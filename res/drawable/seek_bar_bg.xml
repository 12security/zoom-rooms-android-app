<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffd3d3d3" android:endColor="#ffcccccc" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffff0000" android:endColor="#ffff0000" android:angle="270.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffa0ffff" android:endColor="#ffa0ffff" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>